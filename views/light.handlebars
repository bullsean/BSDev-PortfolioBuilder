<body>
    <div id="index-banner" class="parallax-container">
        <div class="section no-pad-bot">
            <div class="container animated slideInUp">
                <br><br>
                <h1 class="header center grey-text text-darken-4">
                    {{data.dataValues.ProfileName.dataValues.profileFirstName}}
                    {{data.dataValues.ProfileName.dataValues.profileLastName}}
                </h1>
                <div class="row center">
                    <h5 class="header col s12 grey-text text-darken-3">{{data.dataValues.ProfileName.dataValues.title}}
                    </h5>
                </div>
                <div class="row center">
                    <img src="/uploads/{{data.dataValues.ProfileImage.dataValues.imageName}}"
                        class="responsive-img valign" alt="" style="width: 20vw;">
                </div>
                <div class="row center">
                    <a href="#projects" id="download-button"
                        class="btn-large waves-effect waves-light grey darken-1">Check Me Out!</a>
                </div>
                <br><br>

            </div>
        </div>
        <div class="parallax"><img src="/images/georgie-cobbs-459520-unsplash.jpg" alt="Unsplashed background img 1">
        </div>
    </div>

    <!--   Icon Section   -->
    <div class="container">
        <div class="row" id="projects">
            <div class="col s12">
                <div class="icon-block">
                    <h2 class="center brown-text"><i class="material-icons">settings</i></h2>
                    <h2 class="header center grey-text text-darken-4">Projects</h2>
                </div>
            </div>
        </div>
        <div class="divider"></div>
        <br><br>
        <div class="row animated slideInLeft">
            <div class="col s6">
                <h4 class="center-align projectTitle">{{data.Experiences.[0].comJectName}}</h4>
                <h5>Title/Role: {{data.Experiences.[0].titleRole}}</h5>
                {{debug data.Experiences.[0].comJectName}}
                <p>{{data.Experiences.[0].description}}</p>
                {{!-- <div class="center">
                    <a class="waves-effect waves-light btn-large grey">Link</a>
                </div> --}}
            </div>

            <div class="col s5">
                <img src="/images/jeff-sheldon-3231-unsplash.jpg" class="responsive-img projectImg" alt="First Project">
            </div>
        </div>
        <br>
        <div class="divider"></div>
        <br><br>
        <div class="row">
            <div class="col s5">
                <img src="/images/spacex-549326-unsplash.jpg" class="responsive-img projectImg" alt="First Project">
            </div>
            <div class="col s6">
                <h4 class="center-align projectTitle">{{data.Experiences.[1].comJectName}}</h4>
                <h5>Title/Role: {{data.Experiences.[1].titleRole}}</h5>
                <p>{{data.Experiences.[1].description}}</p>
                {{!-- <div class="center">
                    <a class="waves-effect waves-light btn-large grey">Link</a>
                </div> --}}
            </div>

        </div>
        <br>
        <div class="divider"></div>
        <br><br>
        <div class="row animated slideInLeft">
            <div class="col s6">
                <h4 class="center-align projectTitle">{{data.Experiences.[2].comJectName}}</h4>
                <h5>Title/Role: {{data.Experiences.[2].titleRole}}</h5>
                {{debug data.Experiences.[2].comJectName}}
                <p>{{data.Experiences.[2].description}}</p>
                {{!-- <div class="center">
                    <a class="waves-effect waves-light btn-large grey">Link</a>
                </div> --}}
            </div>

            <div class="col s5">
                <img src="/images/jo-szczepanska-57782-unsplash.jpg" class="responsive-img projectImg"
                    alt="First Project">
            </div>
        </div>
        <br>
        <div class="divider"></div>
    </div>
    </div>
    </div>
    </div>

    <div class="parallax-container valign-wrapper">
        <div class="section no-pad-bot">
            <div class="container">
                <div class="row">
                    <div class="col s12 center">
                        <h3><i class="mdi-content-send brown-text"></i></h3>
                        <h2 class="grey-text">My Skills & Accomplishments</h2>
                        <div class="row white black-text" style="border-radius: 25px;">
                            {{#each data.dataValues.Skaccoms}}
                            <div class="col s3">
                                <p>{{dataValues.skaccomName}}</p>
                            </div>
                            {{/each}}
                        </div>
                        <h2 class="grey-text">Certificates & Licenses</h2>
                        <div class="row white black-text" style="border-radius: 25px;">
                            {{#each data.dataValues.Licerts}}
                            <div class="col s4">
                                <p>{{dataValues.licertName}}</p>
                            </div>
                            {{/each}}
                        </div>
                        <h2 class="grey-text">Education</h2>
                        <div class="row white black-text" style="border-radius: 25px;">
                            {{#each data.dataValues.Education}}
                            <div class="col s6">
                                <p>{{dataValues.institution}}</p>
                            </div>
                            <div class="col s6">
                                <p>{{dataValues.degree}}</p>
                            </div>
                            {{/each}}
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="parallax"><img src="/images/leone-venter-559377-unsplash.jpg" alt="Unsplashed background img 2">
        </div>
    </div>

    <div class="container">
        <div class="section">

            <div class="row">
                <div class="col s12 center">
                    <h2>Connect</h2>
                    <h1>
                        <a id="logo" href="{{data.dataValues.ConnectLink.dataValues.facebook}}"><i
                                class="fab fa-facebook-square"></i></a>
                        <a id="logo" href="{{data.dataValues.ConnectLink.dataValues.instagram}}"><i
                                class="fab fa-instagram"></i></a>
                        <a id="logo" href="{{data.dataValues.ConnectLink.dataValues.linkedin}}"><i
                                class="fab fa-linkedin"></i></a>
                        <a id="logo" href="{{data.dataValues.ConnectLink.dataValues.github}}"><i
                                class="fab fa-github-square"></i></a>
                    </h1>
                </div>
            </div>

        </div>
    </div>

    <footer class="page-footer grey">
        <div class="container">
            <div class="row">
                <div class="col s12">
                    <h5 class="white-text">Bullock Seyam Developers</h5>
                    <p class="grey-text text-lighten-4">We are a duo of part-time Coding Bootcamp Freelancers working on
                        this project like it's our
                        full time job. If you have any suggestions, would like to collaborate on a layout or wish to
                        enter
                        our design competition, please contact us at <a href=""
                            class="blue-text text-darken-4">info@gmail.com</a></p>
                </div>
            </div>
        </div>
        <div class="footer-copyright">
            <div class="container">
                Made by <a class="blue-text text-darken-4" href="">BSD</a>
            </div>
        </div>
    </footer>

    <!-- materialize JavaScript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
    <!-- JQuery.form library -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.form/3.51/jquery.form.min.js"></script>
    <!--Templates Javascript File-->
    <script type="text/javascript" src="/js/main.js"></script>

</body>